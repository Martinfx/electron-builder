{
  "extends": "./tsconfig-scheme.json",
  "jsdoc": {
    "out": "../../scripts/jsdoc/out/builder-lib",
    "examples": "../../scripts/jsdoc/examples",
    "access": "public"
  },
  "compilerOptions": {
    "outDir": "out",
    "rootDir": "src",
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ]
  },
  "include": [
      "src/**/*.ts",
      "typings/index.d.ts",
      "../typings/index.d.ts",
      "typings/*.d.ts",
      "../../typings/*.d.ts"
  ],
  "exclude": [
  ],
  "references": [
    {
      "path": "../builder-util"
    },
    {
      "path": "../builder-util-runtime"
    },
    {
      "path": "../electron-publish"
    }
  ]
}